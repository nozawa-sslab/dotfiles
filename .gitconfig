[user]
        name = Masanobu Nozawa
        email = masa.nozawa@sslab.ics.keio.ac.jp
[core]
        editor = vim
        pager = diff-so-fancy | less --tabs=4 -RFX
[alias]
        graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
        st = status
        ci = commit -m
        br = branch
        df = diff
        dfn = diff --name-only
        ph = push origin HEAD
        ch = checkout
[interactive]
        diffFilter = diff-so-fancy --patch
[color]
        ui = true
[color "diff-highlight"]
        oldNormal = red bold
        oldHighlight = red bold 52
        newNormal = green bold
        newHighlight = green bold 22
[color "diff"]
        meta = 11
        frag = magenta bold
        func = 146 bold
        commit = yellow bold
        old = red bold
        new = green bold
        whitespace = red reverse
